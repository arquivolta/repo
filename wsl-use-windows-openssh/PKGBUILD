# Maintainer: Your Name <youremail@domain.com>
pkgname=wsl-use-windows-openssh
pkgver=0.1.0
pkgrel=1
pkgdesc=""
arch=('any')
url="https://github.com/arquivolta/wsl-use-windows-openssh"
license=('GPL2')
depends=(make)
makedepends=()
checkdepends=()
optdepends=()
provides=()
conflicts=()
replaces=()
backup=()
options=()
source=("https://github.com/arquivolta/$pkgname/archive/refs/tags/v$pkgver.tar.gz")
noextract=()
sha256sums=('6af577a51c5b1a53849f207a4fd109a5a410710d0024631a8473057dbd6cc5bc')

package() {
	cd "$pkgname-$pkgver"
	make DESTDIR="$pkgdir/" install
}
